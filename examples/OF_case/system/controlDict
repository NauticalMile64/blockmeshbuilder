application     blockMesh;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0;

deltaT          0;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


